#include<iostream>
int main(){
    int num1,num2;
    std::cout<<"enter first integer:";
    std::cin>>num1;
    std::cout<<"enter second integer:";
    std::cin>>num2;
    if(num1==num2){
        std::cout<<"the two numbers are equal";
    }else{
        std::cout<<"the two numbers not equal";
    }
   return 0;
}
